{
  "ver": 2,
  "audioStreaming": true,
  "sources": [
    {
      "center": {TR_CENTER_HZ},
      "rate": {SDR_RATE},
      "error": 0,
      "gain": {GAIN},
      "digitalRecorders": 0,
      "driver": "osmosdr",
      "device": "{SDR_DEVICE}"
    }
  ],
  "systems": [
    {
      "label": "Delta Ambulance",
      "type": "{SYSTEM_TYPE}",
      "channels": [{TR_CHANNELS_HZ}],
      "modulation": "{MODULATION}",
      "squelch": {SQUELCH},
      "analogLevels": {ANALOG_LEVELS}
    }
  ],
  "plugins": [
    {
      "name": "simplestream",
      "library": "libsimplestream.so",
      "streams": [
        {
          "TGID": 0,
          "address": "127.0.0.1",
          "port": 9123,
          "sendJSON": false
        }
      ]
    }
  ]
