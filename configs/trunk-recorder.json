{
  "ver": 2,
  "audioStreaming": true,
  "sources": [
    {
      "center": {TR_CENTER_HZ},
      "rate": {SDR_RATE},
      "error": 0,
      "gain": 40,
      "digitalRecorders": 0,
      "driver": "osmosdr",
      "device": "rtl={SDR_SERIAL}"
    }
  ],
  "systems": [
    {
      "label": "Clinton Fire",
      "type": "{SYSTEM_TYPE}",
      "channels": [{TR_CHANNELS_HZ}],
      "modulation": "{MODULATION}",
      "squelch": -50
    }
  ],
  "plugins": [
    {
      "name": "simplestream",
      "library": "libsimplestream.so",
      "streams": [
        {
          "TGID": 0,
          "address": "127.0.0.1",
          "port": 9123,
          "sendJSON": false
        }
      ]
    }
  ]
}
